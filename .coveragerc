[run]
source = _py2tmp
omit =
  */tests/*
  testing/*
  */testing/*
# Enable branch coverage
branch = True
plugins =
    coverage.plugin

[paths]
source =
    _py2tmp/
    */_py2tmp
[report]
# Regexes for lines to exclude from consideration
exclude_lines =
    pragma: no cover
    raise NotImplementedError
    : \.\.\.
